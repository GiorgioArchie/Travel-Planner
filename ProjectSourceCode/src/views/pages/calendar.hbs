 <div class="container mt-5">
    <h1>Your Trip Calendar</h1>
    <div class="list-group">
        {{#each events}}
        <a href="#" class="list-group-item list-group-item-action">
            <h5 class="mb-1">{{summary}}</h5>
            <p class="mb-1">Start: {{start}}</p>
            <small>End: {{end}}</small>
        </a>
        {{/each}}
    </div>
</div>